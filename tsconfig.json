{
  "compilerOptions": {
    "target": "ES2021",
    "module": "Node16",
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "Node16",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  }
}